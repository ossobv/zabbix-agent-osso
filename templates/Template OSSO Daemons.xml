                <item>
                    <uuid>5cda3c6aa36d4d59b18cc706e896fc4b</uuid>
                    <name>Server certificates expiry days</name>
                    <type>ZABBIX_ACTIVE</type>
                    <key>daemon.cert.minexpiry</key>
                    <delay>1h</delay>
                    <history>1w</history>
                    <value_type>FLOAT</value_type>
                    <units>days</units>
                    <preprocessing>
                        <step>
                            <type>DISCARD_UNCHANGED_HEARTBEAT</type>
                            <parameters>
                                <parameter>1d</parameter>
                            </parameters>
                        </step>
                    </preprocessing>
                    <tags>
                        <tag>
                            <tag>Application</tag>
                            <value>Daemon</value>
                        </tag>
                    </tags>
                    <triggers>
                        <trigger>
                            <uuid>caecd8d25880496d851863446e404d4e</uuid>
                            <expression>({$HAS_DISASTER}=1 or {$HAS_DISASTER}=&quot;9to5&quot;) and time()&gt;{$OFFICE_HOURS_BEGIN} and time()&lt;{$OFFICE_HOURS_END} and last(/Template OSSO Daemons/daemon.cert.minexpiry)&lt;{$CERT_EXPIRY_CRIT}</expression>
                            <name>Certificate(s) about to expire on {HOST.NAME} (&lt;5 days)</name>
                            <opdata>{ITEM.LASTVALUE} left</opdata>
                            <priority>DISASTER</priority>
                            <description>One or more certificates will soon expire. Log onto the host and run cert-expiry-finder for details.</description>
                        </trigger>
                        <trigger>
                            <uuid>fdf47ce7a07940339b30552b588aeeee</uuid>
                            <expression>last(/Template OSSO Daemons/daemon.cert.minexpiry)&lt;{$CERT_EXPIRY_HIGH}</expression>
                            <name>Certificate(s) about to expire on {HOST.NAME} ({ITEM.LASTVALUE} days left)</name>
                            <opdata>{ITEM.LASTVALUE} left</opdata>
                            <priority>HIGH</priority>
                            <description>One or more certificates will soon expire. Log onto the host and run cert-expiry-finder for details.</description>
                        </trigger>
                    </triggers>
                </item>
