    <templates>
        <template>
            <uuid>5fb1060cf5a2426e98238808c7401bb7</uuid>
            <template>Template k8s-patroni postgres status</template>
            <name>Template k8s-patroni postgres status</name>
            <groups>
                <group>
                    <name>Templates/Operating systems</name>
